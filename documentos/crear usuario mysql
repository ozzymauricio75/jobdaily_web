
CREATE USER 'desarrollo'@'%' IDENTIFIED BY 'desarrollo';

GRANT ALL PRIVILEGES ON *.* TO 'desarrollo'@'localhost' IDENTIFIED BY 'desarrollo' WITH GRANT OPTION
MAX_QUERIES_PER_HOUR 0 MAX_CONNECTIONS_PER_HOUR 0 MAX_UPDATES_PER_HOUR 0 MAX_USER_CONNECTIONS 0;

GRANT ALL PRIVILEGES ON *.* TO 'desarrollo'@'%' IDENTIFIED BY 'desarrollo' WITH GRANT OPTION
MAX_QUERIES_PER_HOUR 0 MAX_CONNECTIONS_PER_HOUR 0 MAX_UPDATES_PER_HOUR 0 MAX_USER_CONNECTIONS 0;

CREATE DATABASE IF NOT EXISTS `desarrollo` ;

GRANT ALL PRIVILEGES ON `desarrollo` . * TO 'desarrollo'@'%';

